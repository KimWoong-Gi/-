<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<typeAliases>
		<typeAlias type="com.woong.biz.user.UserVO" alias="user"></typeAlias>
		<typeAlias type="com.woong.biz.movie.MovieVO" alias="movie"></typeAlias>
		<typeAlias type="com.woong.biz.actor.ActorVO" alias="actor"></typeAlias>
